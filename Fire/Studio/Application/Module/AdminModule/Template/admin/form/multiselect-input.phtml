<label>{{label}}</label>
<div class="checkbox-input">
    {{#options}}
    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="{{value}}" id="{{property}}" name="{{property}}[]"{{#selected}} checked=""{{/selected}}>
        <label class="form-check-label" for="{{property}}">
            {{label}}
        </label>
    </div>
    {{/options}}
</div>